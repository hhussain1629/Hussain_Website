        return true;
    var sadends = [4, 16, 37, 58, 89, 145, 42, 20];
    var digit = 0;
    var smsq = 0;
    var numc = num;
    while (2 == 2) {
        while (numc > 0) {
            digit = numc % 10;
            smsq += digit * digit;
            numc = (numc - digit) / 10;
        }
        if (smsq == 1)
