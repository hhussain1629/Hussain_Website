<div class="modal fade" id="fac" tabindex="-1" role="dialog" aria-labelledby="myModalLabel3" aria-hidden="true">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                <h1 class="modal-title" id="myModalLabel3">Factorial</h1>
            </div>
            <div class="modal-body col-sm-12">
                <p>
                    A factorial is of a positive integer, and is calculated by multiplying all positive integers leading up to and including a given positive integer.
                    <br /><br />
                    For example, the factorial of 4 is: 4! = 4 x 3 x 2 x 1 =  24. <br> <br>
                    Please enter a positive integer to get its factorial.<br>
                    <input type="number" id="ip3">
                </p>

                <p>
                    <input type="button" value="Submit" onclick="factorial()">
                </p>



                <p id="op3">

                </p>


                <script>
                    function readText3() {
                        flag = 0;
                        var num = parseFloat($("#ip3").val());
                        if (isNaN(num) || (num < 1) || (num != Math.floor(num)))
                            flag = 1;
                        var package = [flag, num];
                        return package;
                    }

                    function writeText3(f, number, fac) {
                        if (f == 1)
                            ops3 = "The input is invalid. Please enter a positive integer only.";
                        else {
                            ops3 = "The factorial of " + String(number) + " is " + String(fac) + ".";
                        }
                        $("#op3").text(ops3);
                    }

                    function factorial() {
                        var pack = readText3();
                        var fl = pack[0];
                        var n = pack[1];
                        if (fl == 0) {
                            var result = 1;
                            for (x = 1; x <= n; x++)
                                result *= x;
                        }
                        writeText3(fl, n, result);
                    }

                </script>

                <pre class="brush: js text">
function readText3() {
    flag = 0;
    var num = parseFloat($("#ip3").val());
    if (isNaN(num) || (num < 1) || (num != Math.floor(num)))
        flag = 1;
    var package = [flag, num];
    return package;
}
function writeText3(f, number, fac) {
    if (f == 1)
        ops3 = "The input is invalid. Please enter a positive integer only.";
    else {
        ops3 = "The factorial of " + String(number) + " is " + String(fac) + ".";
    }
    $("#op3").text(ops3);
}
function factorial() {
    var pack = readText3();
    var fl = pack[0];
    var n = pack[1];
    if (fl == 0) {
        var result = 1;
        for (x = 1; x <= n; x++)
            result *= x;
    }
    writeText3(fl, n, result);
}
                                </pre>


            </div>
            <div class="modal-footer no-margin">
            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="pal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel4" aria-hidden="true">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                <h1 class="modal-title" id="myModalLabel4">Palindrome</h1>
            </div>
            <div class="modal-body col-sm-12">
                <p>
                    A palindrome is a string, such as &ldquo;level&rdquo;, that is spelled the same way forward and backward.
                    <br> <br>
                    Please enter a string to determine if it is a palindrome.<br>
                    <textarea id="ip4"> </textarea>
                </p>

                <p>
                    <button onclick="palindrome()">Submit</button>
                </p>

                <p>
                    True or False: This string is a palindrome. <br>
                    <span id="op4"> </span>
                </p>

                <script>
                    function palindrome() {
                        var instr = $("#ip4").val();
                        var rev = "";
                        var len = instr.length - 1;
                        for (x = len; x >= 0; x--)
                            rev += instr.charAt(x);
                        result = (instr == rev);
                        $("#op4").text(String(result).toUpperCase());
                    }
                </script>

                <pre class="brush: js text">
function palindrome() {
    var instr = $("#ip4").val();
    var rev = "";
    var len = instr.length - 1;
    for (x = len; x >= 0; x--)
        rev += instr.charAt(x);
    result = (instr == rev);
    $("#op4").text(String(result).toUpperCase());
}
                                </pre>

            </div>
            <div class="modal-footer no-margin">
            </div>
        </div>
    </div>
</div>

